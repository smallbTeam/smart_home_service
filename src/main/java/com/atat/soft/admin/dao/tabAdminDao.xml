<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.atat.soft.admin.dao.TabAdminDao">

    <insert id="addTabAdmin" parameterType="com.atat.soft.admin.bean.TabAdmin">
        INSERT INTO tab_admin
        <trim prefix="(" suffix=")" suffixOverrides=",">
                <if test="mobelPhone != null">
                    mobelPhone,
                </if>
                <if test="password != null">
                    password,
                </if>
                <if test="nickName != null">
                    nickName,
                </if>
                <if test="permissionLevel != null">
                    permissionLevel,
                </if>
                <if test="token != null">
                    token,
                </if>
                <if test="createdDate != null">
                    createdDate,
                </if>
                <if test="createdDate != null">
                    modifiedDate,
                </if>
                    isDeleted
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
                <if test="mobelPhone != null">
                    #{mobelPhone},
                </if>
                <if test="password != null">
                    #{password},
                </if>
                <if test="nickName != null">
                    #{nickName},
                </if>
                <if test="permissionLevel != null">
                    #{permissionLevel},
                </if>
                <if test="token != null">
                    #{token},
                </if>
                <if test="createdDate != null">
                    #{createdDate},
                </if>
                <if test="createdDate != null">
                    #{createdDate},
                </if>
                0
        </trim>
    </insert>

    <update id="updateTabAdminById" parameterType="com.atat.soft.admin.bean.TabAdmin">
        update tab_admin
        <trim prefix="set" suffixOverrides=",">
                <if test="mobelPhone != null">
                    mobelPhone =#{mobelPhone},
                </if>
                <if test="password != null">
                    password =#{password},
                </if>
                <if test="nickName != null">
                    nickName =#{nickName},
                </if>
                <if test="permissionLevel != null">
                    permissionLevel =#{permissionLevel},
                </if>
                <if test="token != null">
                    token =#{token},
                </if>
                <if test="modifiedDate != null">
                    modifiedDate =#{modifiedDate},
                </if>
                <if test="isDeleted != null">
                    isDeleted =#{isDeleted},
                </if>
        </trim>
        where id = #{tabAdminId}
    </update>

    <select id="selectTabAdminList" parameterType="java.util.Map"
            resultType="java.util.Map">
        select
            mobelPhone mobelPhone,
            password password,
            nickName nickName,
            permissionLevel permissionLevel,
            token token,
            createdDate createdDate,
            modifiedDate modifiedDate,
            isDeleted isDeleted,
        id tabAdminId
        from tab_admin
        WHERE
        isDeleted = 0
        <if test="tabAdminId != null">
            and id = #{tabAdminId}
        </if>
        <if test="mobelPhone != null">
            and mobelPhone like #{mobelPhone}
        </if>
        <if test="password != null">
            and password like #{password}
        </if>
        <if test="nickName != null">
            and nickName like #{nickName}
        </if>
        <if test="permissionLevel != null">
            and permissionLevel = #{permissionLevel}
        </if>
        <if test="token != null">
            and token like #{token}
        </if>
        <if test="createdDate != null">
            and createdDate = #{createdDate}
        </if>
        <if test="modifiedDate != null">
            and modifiedDate = #{modifiedDate}
        </if>
        ORDER BY id ASC
    </select>

    <delete id="delTabAdminById" parameterType="java.lang.Integer">
        DELETE FROM  tab_admin
        where
        id = #{tabAdminId}
    </delete>

</mapper>
